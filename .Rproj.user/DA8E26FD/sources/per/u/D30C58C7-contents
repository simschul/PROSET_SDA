S <- io_table$year0$S
Y <- io_table$year0$Y
L <- io_table$year0$L
A <- io_table$year0$A


L_ind <- L
L_ind[3,] <- 0
L_el <- io_table$year0$L - L_ind


emission_calculator(list(S, L, Y))
S_ind <- emission_calculator(list(S, L_ind, Y))
S_el <- emission_calculator(list(S, L_el, Y))

S_ind + S_el

x0 <- list("S" = S,"L" = L, "Y" = Y)
x1 <- io_table$year1[c("S", "L", "Y")]

sda_fun(x0, x1, type = "LMDI1", return = "detail")

A %*% A %>% 
  .[3,]
